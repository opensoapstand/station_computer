[Unit]
Description = Reverse ssh tunnel
After=network.target

[Service]
ExecStart = /usr/bin/ssh -NTC -o ServerAliveInterval=60 -o ExitOnForwardFailure=yes -o StrictHostKeyChecking=no -R 43084:localhost:22 -i /home/df-admin/.ssh/DrinkfillAWS.pem ubuntu@ec2-44-225-153-121.us-west-2.compute.amazonaws.com

Restart = on-failure
RestartSec = 5

[Install]
WantedBy = multi-user.target

