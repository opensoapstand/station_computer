<?xml version="1.0" ?>
<DRINKFILL>
    <!-- Machine Information -->
    <machine>
        <type>df001</type>
        <id>808</id>
    </machine>
    <!-- Hardware Address Reference -->
    <hardware>
        <!-- Dispense Button -->
        <button>
            <type>mcp</type>
            <i2caddress>20</i2caddress>
            <io>00</io>
        </button>
        <!-- Cassette Dispenser Sorted alphabetically (Option 0 = a; 1 = b ...) -->
        <!-- Solenoid index 0 = Drink; 1 = Water; 2 = Air-->
        <!-- TODO: Link these to stateinit.cpp -->
        <!-- Sensors -->

        <!-- Carbonated Drink 1 :: C1 -->
        <dispenser id="0">
            <!-- Drink -->
            <solenoid index="0">
                <type>mcp</type>
                <i2caddress>20</i2caddress>
                <io>01</io>
            </solenoid>
            <!-- Water -->
            <solenoid index="1">
                <type>mcp</type>
                <i2caddress>20</i2caddress>
                <io>02</io>
            </solenoid>
            <!-- Air -->
            <solenoid index="2">
                <type>mcp</type>
                <i2caddress>20</i2caddress>
                <io>03</io>
            </solenoid>
            <flowsensor>
                <type>x86</type>
                <io>337</io>
            </flowsensor>
        </dispenser>
	
	<!-- Carbonated Drink 2 :: C3 -->
	 <dispenser id="1">
            <!-- Drink -->
            <solenoid index="0">
                <type>mcp</type>
