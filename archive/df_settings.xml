                <type>mcp</type>
                <i2caddress>20</i2caddress>
                <io>10</io>
            </solenoid>
            <flowsensor>
                <type>x86</type>
                <io>412</io>
            </flowsensor>
        </dispenser>

        <!-- Carbonated Drink 3 :: C5 -->
        <dispenser id="2">
            <!-- Drink -->
            <solenoid index="0">
                <type>mcp</type>
                <i2caddress>20</i2caddress>
                <io>14</io>
            </solenoid>
            <!-- Water -->
            <solenoid index="1">
                <type>mcp</type>
                <i2caddress>20</i2caddress>
                <io>15</io>
            </solenoid>
            <!-- Air -->
            <solenoid index="2">
                <type>mcp</type>
                <i2caddress>21</i2caddress>
                <io>00</io>
            </solenoid>
            <flowsensor>
                <type>x86</type>
                <io>415</io>
            </flowsensor>
        </dispenser>

        <!-- Carbonated Drink 4 :: C7 -->
        <dispenser id="3">
            <!-- Drink -->
            <solenoid index="0">
                <type>mcp</type>
                <i2caddress>21</i2caddress>
                <io>04</io>
            </solenoid>
            <!-- Water -->
            <solenoid index="1">
                <type>mcp</type>
                <i2caddress>21</i2caddress>
                <io>05</io>
            </solenoid>
            <!-- Air -->
            <solenoid index="2">
                <type>mcp</type>
                <i2caddress>21</i2caddress>
                <io>06</io>
            </solenoid>
            <flowsensor>
                <type>x86</type>
                <io>417</io>
            </flowsensor>
        </dispenser>
  
